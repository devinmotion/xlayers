{
  "compileOnSave": false,
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "",
    "rootDir": "./",
    "outDir": "./dist/out-tsc/src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "esModuleInterop": true,
    "lib": ["esnext", "es2017", "dom"],
    "paths": {
      "@env/*": ["src/environments/*"],
      "@app/*": ["src/app/*"],
      "@xlayers/sketch-ingestor": ["dist/sketch-ingestor"],
      "@xlayers/sketch-ingestor/*": ["dist/sketch-ingestor/*"],
      "@xlayers/sketch-lib": ["dist/sketch-lib"],
      "@xlayers/sketch-lib/*": ["dist/sketch-lib/*"],
      "@xlayers/css-blocgen": ["dist/css-blocgen"],
      "@xlayers/css-blocgen/*": ["dist/css-blocgen/*"],
      "@xlayers/svg-blocgen": ["dist/svg-blocgen"],
      "@xlayers/svg-blocgen/*": ["dist/svg-blocgen/*"],
      "@xlayers/web-blocgen": ["dist/web-blocgen"],
      "@xlayers/web-blocgen/*": ["dist/web-blocgen/*"]
    }
  }
}
