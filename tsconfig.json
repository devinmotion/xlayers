{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "paths": {
      "@env/*": ["src/environments/*"],
      "@app/*": ["src/app/*"],
      "sketchapp-parser": ["dist/sketchapp-parser"],
      "sketchapp-parser/*": ["dist/sketchapp-parser/*"],
      "sketch-blocgen": ["dist/sketch-blocgen"],
      "sketch-blocgen/*": ["dist/sketch-blocgen/*"],
      "std-blocgen": ["dist/std-blocgen"],
      "std-blocgen/*": ["dist/std-blocgen/*"],
      "css-blocgen": ["dist/css-blocgen"],
      "css-blocgen/*": ["dist/css-blocgen/*"],
      "text-blocgen": ["dist/text-blocgen"],
      "text-blocgen/*": ["dist/text-blocgen/*"],
      "svg-blocgen": ["dist/svg-blocgen"],
      "svg-blocgen/*": ["dist/svg-blocgen/*"],
      "vue-blocgen": ["dist/vue-blocgen"],
      "vue-blocgen/*": ["dist/vue-blocgen/*"],
      "bitmap-blocgen": ["dist/bitmap-blocgen"],
      "bitmap-blocgen/*": ["dist/bitmap-blocgen/*"]
    }
  }
}
